<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="765.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.UsersController">
    <children>
        <Pane prefHeight="600.0" prefWidth="765.0" style="-fx-background-color: #141414;">
            <children>
                <Label layoutX="20.0" layoutY="53.0" text="Учетные записи" textFill="#e1e1e1">
                    <font>
                        <Font name="Arial" size="26.0" />
                    </font>
                </Label>
                <Button fx:id="addBtn" layoutX="242.0" layoutY="53.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="135.0" stylesheets="@../../css/style.css" text="Добавить" />
                <TableView fx:id="tb" layoutX="7.0" layoutY="113.0" prefHeight="498.0" prefWidth="752.0" stylesheets="@../../css/styling.css">
                    <columns>
                        <TableColumn fx:id="tcID" prefWidth="61.0" text="ID" />
                        <TableColumn fx:id="tcLogin" prefWidth="136.0" text="Логин" />
                        <TableColumn fx:id="tcPass" prefWidth="305.0" text="Пароль" />
                        <TableColumn fx:id="tcGroup" prefWidth="107.0" text="Группа" />
                        <TableColumn fx:id="tcBlock" minWidth="0.0" prefWidth="156.0" text="Блокировка" />
                    </columns>
                </TableView>
            <Button fx:id="editBtn1" layoutX="397.0" layoutY="53.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="161.0" stylesheets="@../../css/style.css" text="Редактировать" />
            <Button fx:id="delBtn" layoutX="577.0" layoutY="53.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="135.0" stylesheets="@../../css/style.css" text="Удалить" />
            </children>
        </Pane>
    </children>
</AnchorPane>
